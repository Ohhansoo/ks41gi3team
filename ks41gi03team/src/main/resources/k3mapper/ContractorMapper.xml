	<select id="k3GetContractorList" resultType="K3Contractor">
		SELECT
			c.contractorCode
			,cs.contractorName
			,c.contractorItem
			,c.contractorStatus
			,c.contractorDevision
			,cs.contractorPhone
			,c.contractorRegistrationDate
			,c.contractorId
			,c.contractorState
			,c.memberId
		FROM
			k3_tb_contractor AS c
			INNER join
			k3_tb_contractor_staff AS cs
			on
			c.contractorId = cs.contractorId;
	</select>

</mapper>